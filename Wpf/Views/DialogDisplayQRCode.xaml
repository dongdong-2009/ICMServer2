<Window x:Class="ICMServer.WPF.Views.DialogDisplayQRCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICMServer.WPF.Views"
        mc:Ignorable="d"
        Title="QR Code" Height="Auto" Width="Auto" 
        WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow" SizeToContent="WidthAndHeight" ResizeMode="NoResize" FontSize="14">
    <Image Source="{Binding QRCode}" Stretch="UniformToFill" Width="300" Height="300"/>
</Window>
